{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2019,
    "ecmaFeatures": {
      "impliedStrict": true
    },
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2017": true
  },
  "rules": {
    "no-unused-vars": ["error", {
      "ignoreRestSiblings": true
    }],
    "react/prop-types": "off"
  }
}
